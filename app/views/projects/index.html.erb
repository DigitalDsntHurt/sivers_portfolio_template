<p id="notice"><%= notice %></p>



<h1>Projects</h1>
<%= link_to 'New Project', new_project_path %>
<table class="scaffold-index">
  <thead>
    <tr>
      <th>Title</th>
      <th>Portfolio group</th>
      <th>Role</th>
      <th>Organization</th>
      <th>Industry</th>
      <th>Project type</th>
      <th>Skills</th>
      <th>Technologies</th>
      <th>Summary</th>
      <th>Organization context</th>
      <th>Project context</th>
      <th>Problem</th>
      <th>Solution parameters</th>
      <th>Process</th>
      <th>Solution</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= link_to project.title, project %></td>
        <td><%= project.portfolio_group %></td>
        <td><%= project.role %></td>
        <td><%= project.organization %></td>
        <td><%= project.industry %></td>
        <td><%= project.project_type %></td>
        <td><%= project.skills %></td>
        <td><%= project.technologies %></td>
        <td><%= project.summary %></td>
        <td><%= project.organization_context %></td>
        <td><%= project.project_context %></td>
        <td><%= project.problem %></td>
        <td><%= project.solution_parameters %></td>
        <td><%= project.process %></td>
        <td><%= project.solution %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


